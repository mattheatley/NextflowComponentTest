
/* nextflow run ... -with-report [file_name] */ 

    report {
        enabled   = true
        overwrite = true
        file      = 'Report.txt'
    }

/* nextflow run ... -with-trace */ 

    trace {
        enabled   = true
        overwrite = true
        file      = 'Trace.txt'
    }

/* nextflow run ... -with-timeline [file_name] */ 

    timeline {
        enabled   = true
        overwrite = true
        file      = 'Timeline.txt'
    }


/* nextflow run ... -with-dag [file_name] */ 

    dag {
        enabled   = true
        overwrite = true
        file      = 'Dag.dot' // <dot|html|mmd|pdf|png|svg|gexf>
    }